		<script type="text/javascript"
			src="http://i1.sdlcdn.com/js/136565DF521B6D06C659C87DB16D38DB/jquery/jquery-ui-1.8.16-slider.js"></script>
		<script type="text/javascript">
		var noSpin = false;
		$.fn.stars = function() {
			return $(this).each(function() {
				$(this).html($('<span/>').width(Math.max(0, (Math.min(5, parseFloat($(this).html())))) * 16));
			});
		}
		$('span.stars').stars();
		var selectedView = $("#selectedViewType").html();
		var reviewFlag = true;
		var compareFlag = $('#compareFlag').val();
		var emiThreshold = $('#emiThreshold').val();
		var clickSrcValue = "go_header";
		function removeProductLastBor(){
			$('.grid_row_height').each(function(){
				$(this).append('<div class="lclear" />'); 
				var maxHeight = $(this).height();
				$(this).find(".product_grid_cont").css('height', maxHeight);
				$(this).children().last().prev().css('border-right','0px');
				var maxHeight = 0;
				$(this).removeClass('grid_row_height');
			});
		}	
		var cpbLen = 0;
		function addToCompareClickList(obj){
			var prodId = $(obj).attr('id').split('-')[1];
			var prodName = $('#'+prodId).children().eq(0).children().children().eq(0).children().attr('alt');
			var prodImage =  $('#'+prodId).children().eq(0).children().children().eq(0).children().attr('src');
			if($(obj).is(':checked')){
				addToCompareBox(prodId,prodName,prodImage,true);
				comparedProdData();
			}else{
				removeAddToCompareBox(prodId);
			}
		}
		function addToCompareClick(obj){
			var prodId = $(obj).attr('id').split('-')[1];
			var prodName = $('#'+prodId).children().children().eq(0).children().attr('alt');
			var prodImage =  $('#'+prodId).children().children().eq(0).children().attr('src');
			if($(obj).is(':checked')){
				addToCompareBox(prodId,prodName,prodImage,true);
				comparedProdData();
			}else{
				removeAddToCompareBox(prodId);
			}
		}
		function addToCompareBox(prodId,prodName,prodImage,visibleFlag){
			if (cpbLen > 3){
				$('#compareProdError').show();
				$('#checkboxAddCompare-'+prodId).attr('checked',false);
				$('#compareProductsOuter').show();
				return false;
			}
			if (visibleFlag ==true){
				$('#compareProductsOuter').show();
			}
			$('#'+prodId).addClass('addCompareProdBox');
			$('.box').eq(cpbLen).removeClass('empty').addClass('compareProduct').attr('id','compareProductBox-'+prodId).html(addToCompareHtml(prodId,prodName,prodImage));
			cpbLen++;
			if(cpbLen >=2){
				$('#compareButtonOuter').removeClass('disable');
			}
		}
		function removeAddToCompareBox(prodId){
			$('#compareProductsOuter').show();
			$('#compareProdError').hide();
			$('#compareProductBox-'+prodId).remove();
			$('#compareProductsBox').append('<div class="lfloat overhid box empty"></div>');
			$('#checkboxAddCompare-'+prodId).attr('checked',false);
			$('#'+prodId).removeClass('addCompareProdBox');
			comparedProdData();
			cpbLen--;
			if(cpbLen < 2){
				$('#compareButtonOuter').addClass('disable');
			}	
		}
		function addToCompareHtml(prodId,prodName,prodImage) {
			prodImage = prodImage.replace('166x194', 'small');
			var html = '';
			html += '<div class="compareProd-close disable" onClick="removeAddToCompareBox('+prodId+');"></div>';
			html += '<div class="lfloat">';
			html += 	'<img src="'+prodImage+'" width="48"/>';
			html +=  '</div>';
			html +=  '<div class="lfloat prodTitle overhid">'+prodName+'</div>';
			return html;
		};
		function comparedProdList(){
			var comparedProdIds ='';
			$('.compareProduct').each(function(){
				comparedProdIds += $(this).attr('id').split('-')[1]+',';
			});
			return comparedProdIds;
		}
		$('#compareProductsOuter-close').click(function(){
			$('#compareProductsOuter').hide();
			comparedProdIds = comparedProdList();
			var ajaxUrl = "/json/insert/"+$('#prodTypeId').html()+"?pogIds="+comparedProdIds+"visibleFlag0";
			$.ajax({
				url : ajaxUrl,
				type : "GET",
				dataType : 'json',
				success : function() {
				}
			});
		});
			$('.compareProducts-button').click(function(){
				if(cpbLen >=2){
					comparedProdIds = comparedProdList();
					comparedProdIds = comparedProdIds.substring(0,comparedProdIds.lastIndexOf(","));
					window.location.href = '/product/books/compare?ids='+comparedProdIds;
				}
			});
			$('.compareProducts-resetLink').click(function(){
				if(cpbLen >=2){
					$('#compareButtonOuter').addClass('disable');
					$('#compareProductsBox').html('<div class="lfloat overhid box empty" /><div class="lfloat overhid box empty" /><div class="lfloat overhid box empty" /><div class="lfloat overhid box empty" />');
					$('.product_grid_cont, .product_list_view_cont').removeClass('addCompareProdBox');
					$('.addToCompare').children().attr('checked',false);
					$('#compareProdError').hide();
					comparedProdData();
				cpbLen = 0;
				}
			});
			$("input[type=checkbox][filterType=checkbox]").click(function() {
				if($(this).is(":checked")) {
					var param = $(this).attr('name')+" : "+$(this).attr('value');
					updateOmniture("changedFilter","FID", param);
				}
			});
			$("input[type=radio][filterType=radio]").click(function() {
				if($(this).is(":checked")){
					var param = $(this).attr('name')+" : "+$(this).attr('value');
					updateOmniture("changedFilter","FID", param);
				}
			});
			function comparedProdData() {
				comparedProdIds = comparedProdList();
				var ajaxUrl = "/json/insert/"+$('#prodTypeId').html()+"?pogIds="+comparedProdIds+"visibleFlag1";
				$.ajax({
					url : ajaxUrl,
					type : "GET",
					dataType : 'json',
					cache: false,
					success : function() {
					}
				});
			}
			function comparedProdGetData() {
				if( compareFlag == "true") {
					var ajaxUrl = "/json/get/"+$('#prodTypeId').html();
					$.ajax({
						url : ajaxUrl,
						type : "GET",
						dataType : 'json',
						cache: false,
						success : function(data) {
							$('#compareHeading').html(data.productCompareWidgetDTO.widgetName);
							if( $.isArray(data.productCompareWidgetDTO.minPOGs) ) {
								displayComparedProducts(data.productCompareWidgetDTO);	
							} else {
								var visibleFlag = data.productCompareWidgetDTO.visible;
								if(data.productCompareWidgetDTO.minPOGs != undefined){
									var prodId = data.productCompareWidgetDTO.minPOGs.id;
									var prodName = data.productCompareWidgetDTO.minPOGs.name;
									var prodImage = Snapdeal.getResourcePath('')+ data.productCompareWidgetDTO.minPOGs.image;
									addToCompareBox(prodId,prodName,prodImage,visibleFlag);
									$('#checkboxAddCompare-'+prodId).attr('checked',true);
								}
							}
						}
					});
				}
			}
			function displayComparedProducts(data){
				var visibleFlag = data.visible;
				if(data.minPOGs != undefined){
					$.each(data.minPOGs, function(index, val){
						var prodId = val.id;
						var prodName = val.name;
						var prodImage = Snapdeal.getResourcePath('')+ val.image;
						addToCompareBox(prodId,prodName,prodImage,visibleFlag);
						$('#checkboxAddCompare-'+prodId).attr('checked',true);
	    			});
				}	
            }
			function hgltComparedProds(){
				if( $('#compareProductsBox').children().hasClass('compareProduct') ) {
					var comparedProdIds = ''; 
					$('.compareProduct').each(function(){
						comparedProdIds += $(this).attr('id').split('-')[1];	
						if(!$('#'+comparedProdIds).hasClass('addCompareProdBox')){
							$('#'+comparedProdIds).addClass('addCompareProdBox');
							$('#checkboxAddCompare-'+comparedProdIds).attr('checked',true);
						}	
						comparedProdIds = ''; 
					});
				}
			}
			 var theLoc = $('#compareProductsOuter').position().top;
			 	$(window).scroll(function() {
			 		if(theLoc >= $(window).scrollTop()) {
			 			if($('#compareProductsOuter').hasClass('fixed')) {
			 				$('#compareProductsOuter').removeClass('fixed');
			 			}
			 		} else { 
			 			if(!$('#compareProductsOuter').hasClass('fixed')) {
			 				$('#compareProductsOuter').addClass('fixed');
			 			}
			 		}
			 	});
			function redirectToPDP(element, url) {
				if( url.indexOf("?") == -1 ) {
					newUrl = url + '?attrVal=' + element.value;
				} else {
					newUrl = url + '&attrVal=' + element.value;
				}
				//alert(newUrl);
				window.location.href = newUrl;
			}
			var onFilter = false;
			function changeFilters() {
				var q="";
				var refinedBoxContent = "";
				$("#filter-no-results-message").hide();
				$('.filter').each(function(){
					var filterName = $(this).attr('name'); 
					var filterType = $(this).attr('filterType');
					var values = "";
					var refineByValues = "";
					if(filterType == "checkbox") {
						$("input:checkbox[name="+filterName+"]:checked").each(function() {
							values += $(this).val() + "^ ";
						});
						if(values.length > 0) {
							values = values.substring(0, values.lastIndexOf("^"))
							refineByValues += values.replace(/\^/g,",");
						}
						onFilter = true;
					} else if(filterType == "radio") {
						values = $("input:radio[name="+filterName+"]:checked").val();
						refineByValues += values;
					} else if (filterType == "slider") {
						values = $("input[name="+filterName+"-slider]").val();
						refineByValues += "Rs. " + values.split(',')[0] + " - Rs."  + values.split(',')[1];
					}
					if(values !== undefined && values.length > 0) {
						q += filterName + ":" + values + "|";
						refinedBoxContent += '<span class="refined-box">'+refineByValues+'</span> <span class="filter-sep">-</span> ';
					}
				});
				q = encodeURIComponent(q);
				if(refinedBoxContent.length > 0) {
					refinedBoxContent = refinedBoxContent.substring(0, refinedBoxContent.lastIndexOf("-"));
					$('#refined-box-content').html("<span class='refined-text'>Refined by: </span>" + "<div class='refined-filters-outer'>" + refinedBoxContent + "</div>");
					$('#refined-box-content').show();
				} else {
					$('#refined-box-content').html("");
					$('#refined-box-content').hide();
				}
				/* if( Snapdeal.CommonUtils.getQueryStringParameter('q') != null ){
					q = encodeURIComponent(Snapdeal.CommonUtils.getQueryStringParameter('q'));
				} */
				$('#seeMoreProducts').attr('start',0).attr('qparam', q);
				if(selectedView=="List"){
					getProducts($('#seeMoreProducts').attr('categoryid'), 0, 5, false, encodeURIComponent(q), $('#seeMoreProducts').attr('sortType'),selectedView);
				}else {
					getProducts($('#seeMoreProducts').attr('categoryid'), 0, 20, false, encodeURIComponent(q), $('#seeMoreProducts').attr('sortType'),selectedView);
				}
			}
			function getProductHtml(productDTO, index, numFound, start) {
				var productHtml = '';
			    var brRite = '';
				if(index == 0 || (index+1)%4 == 1) {
					productHtml += '<div class="grid_row_height product_grid_row">';
				}
				var absPos;
				if(start != null){
					absPos = parseInt(start) + parseInt(index);
				}else{
					absPos = index;
				}
				productHtml += '<div class="product_grid_cont" id="'+productDTO.id+'"><a class="hit-ss-logger" v="p" categoryId="'+$("#categoryId").val()+'" href="'+window.sdHttpPath+'/'+productDTO.pageUrl + '?pos=' + absPos + ';' + numFound + '" title="'+ productDTO.vendorName + '" pogId="'+productDTO.id+'">';
				productHtml += '<div class="product-image">' 
				+ '<img src="'+ window.sdstaticPath + productDTO.image +'" width="166" height="194" border="0" alt="'+ productDTO.vendorName +'"/>'
				+ ((productDTO.soldOut) == true ?'<div class="soldout"></div>':'')
				+ '</div>'
				+ '<div class="product_grid_cont_heading">' + productDTO.vendorName + '</div>';
				if(productDTO.noOfReviews > 0 && reviewFlag == true){
					if(productDTO.noOfReviews > 1){
					productHtml +='<div class="overhid productReviews"><span class="stars lfloat"><span style="width:'+Math.max(0, (Math.min(5, parseFloat(productDTO.avgRating)))) * 16+'px;"/></span><span class="lfloat" style="font-size:11px; padding-left:5px;color:#727272;">| '+productDTO.noOfReviews+' Reviews</span></div>';
					}
					if(productDTO.noOfReviews == 1){
					productHtml +='<div class="overhid productReviews"><span class="stars lfloat"><span style="width:'+Math.max(0, (Math.min(5, parseFloat(productDTO.avgRating)))) * 16+'px;"/></span><span class="lfloat" style="font-size:11px; padding-left:5px;color:#727272;">| '+productDTO.noOfReviews+' Review</span></div>';
					}
				}				
				if(productDTO.freebies!=null &&  productDTO.freebies.length>0){
					productHtml += '<div class="freebies_inside" align="center"><img src="'+window.sdstaticPath+'img/freebie/freebie_inside.png?v=1"></div>';
				}
				productHtml += '<div class="product_grid_cont_price_outer">';
				if(productDTO.price == null && productDTO.voucherPrice == null){
					productHtml+='<div class="fnt12 prod-prebook">Pre Order at <strong class="blackText">Rs ' + productDTO.displayPrice + '</strong></div>';
				}else{
					productHtml+= '<div class="product_price ' + (productDTO.discount > 0 ? '' : 'no-float') + '"><span class="originalprice ' + (productDTO.discount > 0 ? '' : 'no-display') + '">Rs&nbsp;' + productDTO.price + '</span>  Rs&nbsp;' + productDTO.displayPrice + '</div>' ;
				}
				if(productDTO.discount>0){
				productHtml += '<div class="product_discount_outer"><div class="product_discount">' + productDTO.discount + '% <span>Off</span></div></div>';
				}
				productHtml += '</div>'; 
				productHtml += ((productDTO.codValid) == true ?'<div class="product-cod">Cash on delivery available</div>':'')
				+ '</a>';
				var attrs = Snapdeal.Utils.fixArray(productDTO.initAttributes); 
			 	if(productDTO.initAttributes != null && attrs.length > 0) {
					productHtml += '<span class="productAttr"> Select '
					+ attrs[0].name
					+ '<select onChange=" redirectToPDP( this, \' ' + window.sdHttpPath+'/'+productDTO.pageUrl + '?pos=' + absPos + ';' + numFound + '\')">'
					+ '<option value="">--</option>';
					for( i=0; i<attrs.length; i++ ) {
						if( attrs[0].name == attrs[i].name ){
							if(!attrs[i].soldOut && attrs[i].live){
								productHtml += '<option value="' + attrs[i].value + '">' + attrs[i].value + '</option>';
							}
						}
					}
					productHtml += '</select>'
					+ '</span>';
				}
			 	if(compareFlag=='true'){
					productHtml += '<label class="addToCompare overhid"><input type="checkbox" id="checkboxAddCompare-'+productDTO.id+'" class="lfloat" onClick="addToCompareClick(this)"/> <div class="lfloat" style="padding:2px 0 0 1px">Add to compare</div></label>';
				}
				productHtml += '</div>';
				productHtml += ( (index+1)%4 == 0 ) ? '</div>' : ''; 
				return productHtml;
			}
			function displayProducts(data, retainExistingProducts, start) {
				var productHtml = "";
				var productHtml4 = "";
				if (retainExistingProducts) {
					// earlier it was there 
					productHtml += $(".products_wrapper").html();
				} else { $("#products-main4").html(''); }
				var noOfProducts = 0;
				if (data.minProductResponseDTO.status == "Fail") {
					if($('#seeMoreProducts').attr('start') == "0"){
						$(".filter-prod-head-outer, #refined-box-content ").hide();						
					}
				} else if (data.minProductResponseDTO.productDtos.length == undefined) {
					productHtml4 += getProductHtml(data.minProductResponseDTO.productDtos, 1 ,data.minProductResponseDTO.numberFound,start);
					noOfProducts = 1;
					$("#products-main4").append(productHtml4);
					$(".filter-prod-head-outer, #refined-box-content ").show();
					hgltComparedProds();
				} else {
					noOfProducts = data.minProductResponseDTO.productDtos.length;
					for ( var i = 0; i < data.minProductResponseDTO.productDtos.length; i++) {
						productHtml4 += getProductHtml(data.minProductResponseDTO.productDtos[i], i,data.minProductResponseDTO.numberFound,start);
					}
					$(".filter-prod-head-outer ").show();
					$("#products-main4").append(productHtml4);
					removeProductLastBor();
					hgltComparedProds();
					if( $(".refined-text").html() != null ) {
						$("#refined-box-content").show();
					} else {
						$("#refined-box-content").hide();
					}
				}
				var resultsFound = data.minProductResponseDTO.numberFound;
				if(resultsFound != "0") {
					$('#no-of-results-filter').html(resultsFound);
					$('#resultsOnPage').val(resultsFound);
				}
				if($('#seeMoreProducts').attr('categoryid') != 0 && resultsFound == "0" && $('#seeMoreProducts').attr('start') == "0") {
					$('#filter-no-results-message').show();
					$('#productResultsFound').hide();
					$('#no-of-results-filter').html("0");
					$('#resultsOnPage').val("0");
					//getProducts($('#seeMoreProducts').attr('categoryid'), 0, 20, false, '', $('#seeMoreProducts').attr('sortType'));
				} else {
					$('#filter-no-results-message').hide();
					$('#productResultsFound').show();
				}
				//$(".products_wrapper").html(productHtml);
				$('#ajax-loader-icon').hide();
				if (noOfProducts < 20) {
					$('#seemore').hide();
					$('#seeMoreState').val('hidden');
				} else {
					$('#seemore').show();
					$('#seeMoreState').val('visible');
					var start = $('#seeMoreProducts').attr('start');
					var end = $('#seeMoreProducts').attr('end');
					$('#seeMoreProducts').attr('start', parseInt(start) + parseInt(end));
					$('#startProductState').val(parseInt(start) + parseInt(end));
				}
				if(onFilter) {  $("#productResultsFound").hide(); }
			}
			function getProducts(categoryId, start, limit, retainExistingProducts, q, sortType, viewType) {
				var qry = $("#lastKeyword").val();
				if( qry == 'eg : Pizza, Samsung Galaxy, Corbett resort' ) qry = '';
				var prodCatId = $("#prodCatId").val();
				if(!$("#prodCatId")){
				}
				if (prodCatId != '') {
					categoryId = prodCatId;
				}
				if(categoryId == '' || categoryId == null){
					categoryId = "364";
				}
				if(categoryId == '' || categoryId == null){
					categoryId = 0;
				}
				if(viewType == null){
					viewType = "Grid";
				}
				if( !$("#filter-no-results-message").is(":visible") ) {
					var ajaxUrl = "/json/product/get/search/" + categoryId + "/" + start + "/" + limit + "?q="+q+"&sort="+sortType+"&keyword="+qry+"&clickSrc="+clickSrcValue+"&viewType="+viewType;
					$.ajax({
						url : ajaxUrl,
						type : "GET",
						dataType : 'json',
						beforeSend: function() { 
										if( noSpin == false ){ 
											$(".ajax-loader-outer").show();
											$("#products-main4, #products-main4 img").css('filter', 'alpha(opacity=50)').css('opacity', 0.5);
								} 
						},
						success : function(data) {
							if(viewType == 'List'){
								displayListProducts(data, retainExistingProducts, start);
								window.cachePageData.updateProductDto(data, start,"List");
							}else{
								displayProducts(data, retainExistingProducts, start);
								window.cachePageData.updateProductDto(data, start,"Grid");	
							}
							$(".ajax-loader-outer").hide();
							$("#products-main4, #products-main4 img").css('filter', 'alpha(opacity=100)').css('opacity', 1);
							noSpin = false;
						},
						error: function(){
							$(".ajax-loader-outer").hide();
							$('#ajax-loader-icon').hide();
							noSpin = false;
						}
					});
				}
			}
			function displayListProducts(data, retainExistingProducts, start) {
				var productHtml = "";
				var productListHtml = "";
				if (retainExistingProducts) {
					// earlier it was there 
					productHtml += $(".products_wrapper").html();
				} else { $("#products-main4").html(''); }
				var noOfProducts = 0;
				if (data.listViewProductResponseDTO.status == "Fail") {
					if($('#seeMoreProducts').attr('start') == "0"){
						$(".filter-prod-head-outer, #refined-box-content ").hide();						
					}
				} else if (data.listViewProductResponseDTO.productOfferGroupDtos.length == undefined) {
					productListHtml += getListProductHtml(data.listViewProductResponseDTO.productOfferGroupDtos, 1 ,data.listViewProductResponseDTO.numberFound,start);
					noOfProducts = 1;
					$("#products-main4").append(productListHtml);
					$(".filter-prod-head-outer, #refined-box-content ").show();
					hgltComparedProds();
				} else {
					noOfProducts = data.listViewProductResponseDTO.productOfferGroupDtos.length;
					for ( var i = 0; i < data.listViewProductResponseDTO.productOfferGroupDtos.length; i++) {
						productListHtml += getListProductHtml(data.listViewProductResponseDTO.productOfferGroupDtos[i], i,data.listViewProductResponseDTO.numberFound,start);
					}
					$(".filter-prod-head-outer ").show();
					$("#products-main4").append(productListHtml);
					hgltComparedProds();
					if( $(".refined-text").html() != null ) {
						$("#refined-box-content").show();
					} else {
						$("#refined-box-content").hide();
					}
				}
				var resultsFound = data.listViewProductResponseDTO.numberFound;
				if(resultsFound != "0") {
					$('#no-of-results-filter').html(resultsFound);
					$('#resultsOnPage').val(resultsFound);
				}
				if($('#seeMoreProducts').attr('categoryid') != 0 && resultsFound == "0" && $('#seeMoreProducts').attr('start') == "0") {
					$('#filter-no-results-message').show();
					$('#productResultsFound').hide();
					$('#no-of-results-filter').html("0");
					$('#resultsOnPage').val("0");
				} else {
					$('#filter-no-results-message').hide();
					$('#productResultsFound').show();
				}
				$('#ajax-loader-icon').hide();
				if (noOfProducts < 5) {
					$('#seemore').hide();
					$('#seeMoreState').val('hidden');
				} else {
					$('#seemore').show();
					$('#seeMoreState').val('visible');
					var start = $('#seeMoreProducts').attr('start');
					var end = $('#seeMoreProducts').attr('end');
					$('#seeMoreProducts').attr('start', parseInt(start) + parseInt(end));
					$('#startProductState').val(parseInt(start) + parseInt(end));
				}
				if(onFilter) {  $("#productResultsFound").hide(); }
			}
			function getListProductHtml(productDTO, index, numFound, start) {
				var productHtml = '';
			    var brRite = '';
				var absPos;
				if(start != null){
					absPos = parseInt(start) + parseInt(index);
				}else{
					absPos = index;
				}
				productHtml +='<div class="product_list_view_cont overhid" id="'+productDTO.id+'">';
				productHtml += 	 '<div class="lfloat product_image_wrapper overhid">';
				productHtml += 		'<a class="hit-ss-logger" v="p" categoryId="'+$("#categoryId").val()+'" href="'+window.sdHttpPath+'/'+productDTO.pageUrl + '?pos=' + absPos + ';' + numFound + '" title="'+ productDTO.vendorName + '" pogId="'+productDTO.id+'">';
				productHtml += 			'<div class="product-image lfloat">';
				productHtml +=				'<img src="'+ window.sdstaticPath + productDTO.image +'" height="148" border="0" alt="'+ productDTO.vendorName +'" width="100%"/>';
				productHtml += 				((productDTO.soldOut) == true ?'<div class="soldoutListView"></div>':'');
				productHtml += 			'</div>';
				productHtml +=		'</a>';
				if(compareFlag=='true'){
					productHtml +=	'<label class="lfloat addToCompare overhid" style="margin: 5px auto 0 10px;">';
					productHtml +=		'<span style="padding: 5px 0 0 3px">';
					productHtml +=			'<input id="checkboxAddCompare-'+productDTO.id+'" type="checkbox" class="lfloat" onClick="addToCompareClickList(this)" />';
					productHtml +=		'</span>';
					productHtml +=		'<div class="lfloat" style="padding: 5px 0 0 3px">Add to compare</div>';
					productHtml +=	'</label>';
				}
				productHtml +=  '</div>';
				productHtml += 	'<div class="lfloat product_list_view_info_cont">';
				if(productDTO.prebook){
					productHtml += 	'<div class="prebook-tag">Pre Order Product</div>';
				}
				productHtml += 		'<a class="hit-ss-logger" v="p" categoryId="'+$("#categoryId").val()+'" href="'+window.sdHttpPath+'/'+productDTO.pageUrl + '?pos=' + absPos + ';' + numFound + '" title="'+ productDTO.vendorName + '" pogId="'+productDTO.id+'">';
				productHtml += 			'<div class="product_list_view_heading">' + productDTO.vendorName + '</div>';
				productHtml += 		'</a>';	
				if(productDTO.noOfReviews > 0 && reviewFlag == true){
					if(productDTO.noOfReviews > 1){
					productHtml +=	'<div class="overhid productListViewReviews">';
					productHtml +=		'<span class="stars lfloat">';
					productHtml +=			'<span style="width:'+Math.max(0, (Math.min(5, parseFloat(productDTO.avgRating)))) * 16+'px;"/>';
					productHtml +=		'</span>';
					productHtml +=		'<span class="lfloat" style="font-size:11px; padding-left:5px;color:#727272; padding-top:2px;">';
					productHtml += 			'| <a href="'+window.sdHttpPath+'/'+productDTO.pageUrl + '?pos=' + absPos + ';' + numFound + '#ReviewHeader" >'+productDTO.noOfReviews+' Reviews </a>';
					productHtml +=		'</span>';
					productHtml +=	'</div>';
					}
					if(productDTO.noOfReviews == 1){
					productHtml +=	'<div class="overhid productListViewReviews">';
					productHtml +=		'<span class="stars lfloat">';
					productHtml +=			'<span style="width:'+Math.max(0, (Math.min(5, parseFloat(productDTO.avgRating)))) * 16+'px;"/>';
					productHtml +=		'</span>';
					productHtml += 		'<span class="lfloat" style="font-size:11px; padding-left:5px;color:#727272; padding-top:2px;">';
					productHtml +=			'| <a href="'+window.sdHttpPath+'/'+productDTO.pageUrl + '?pos=' + absPos + ';' + numFound + '#ReviewHeader" >'+productDTO.noOfReviews+' Review  </a>';
					productHtml +=		'</span>';
					productHtml +=	'</div>';
					}
				}
				productHtml += 		'<div class="overhid product_list_view_price_outer">';
				if(productDTO.prebook && !productDTO.launched){
					if(productDTO.price != null && productDTO.voucherPrice != null && productDTO.displayPrice !='' && productDTO.voucherPrice == productDTO.displayPrice){
						productHtml +=	'<div class="product_list_view_price lfloat">';
						productHtml +=		'<strike class="'+(productDTO.discount > 0 ? '' : 'no-display')+'">Rs ' + productDTO.price + '</strike>';
						productHtml +=		'&nbsp;<strong>Rs&nbsp;' + productDTO.displayPrice + '</strong>';
						productHtml +=	'</div>';
						productHtml +=	'<div class="product_list_view-discount lfloat '+(productDTO.discount > 0 ? '' : 'no-display')+'">' + productDTO.discount + '%</div>';
						productHtml +=	'<div>&nbsp;</div>';
					}
					if(productDTO.price == null && productDTO.voucherPrice == null && productDTO.displayPrice !=''){
						productHtml +=	'<div class="fnt12" style="color:#383838">MRP : To be announced</div>';
						productHtml +=	'<div class="payinAdv fnt12">';
						productHtml +=		'<div>Pay in Advance :&nbsp;';
						productHtml +=			'<span class="redText advPrice">Rs ' + productDTO.displayPrice + '</span>';
						productHtml +=		'</div>';
						productHtml +=	'</div>';
					}if(productDTO.price != null && productDTO.voucherPrice != null && productDTO.displayPrice !='' && productDTO.voucherPrice != productDTO.displayPrice){
						productHtml +=	'<div class="prodbuy-price lfloat">';
						productHtml +=		'<strike class="'+(productDTO.discount > 0 ? '' : 'no-display')+' original_price_prebook_list_view">';
						productHtml +=			'Rs&nbsp;<span id="original-price-id-' + productDTO.id + '">' + productDTO.price + '</span>';
						productHtml +=		'</strike>';
						productHtml +=		'<span class="selling_price_prebook_list_view">';
						productHtml +=			'<strong>&nbsp;Rs <span id="selling-price-id-' + productDTO.id + '">' + productDTO.voucherPrice + '</span></strong>';
						productHtml +=		'</span>';
						productHtml +=	'</div>';
						productHtml +=	'<div  class="prodbuy-discount '+(productDTO.discount > 0 ? '' : 'no-display')+' lfloat product_list_view-discount">';
						productHtml +=		'<span id="discount-id-' + productDTO.id + '">' + productDTO.discount + '%</span>';
						productHtml +=	'</div>';
						productHtml +=	'<div class="clear"></div>';
						productHtml +=	'<div class="payinAdv fnt12">';
						productHtml +=		'<div>Pay in Advance :&nbsp;';
						productHtml +=			'<span class="redText advPrice">Rs ' + productDTO.displayPrice + '</span>';
						productHtml +=		'</div>';
						productHtml +=	'</div>';
					}
					productHtml += 		'<div class="lfloat prebookDate fnt12" id="prebookDate-'+productDTO.id+'">';
					productHtml += 			'Release Date: <strong>'+productDTO.launchDate+'</strong>';
					productHtml += 		'</div>';
					productHtml += 		'<div class="prebookHelp lfloat" onclick="prebookHelpClick('+productDTO.id+')" onmouseout="prebookHelpLeave('+productDTO.id+')"> </div>';
					productHtml += 		'<div class="lfloat prebookInfoVal" id="prebookInfo-'+productDTO.id+'" style="display:none;">';
					productHtml += 			'<div class="toolTip-box-left-arrow lfloat"></div>';
					productHtml += 			'<div class="toolTip-box lfloat" style="width:200px;">';
					productHtml += 				'<div class="prebookInfoText">'+$('#prebookText').val()+'</div>';
					productHtml += 			'</div>';
					productHtml += 		'</div>';
					productHtml += 		'<div class="clear"></div>';
				}else{
					productHtml +=		'<div class="product_list_view_price lfloat">';
					productHtml +=			'<strike class="'+(productDTO.discount > 0 ? '' : 'no-display')+'">';
					productHtml +=				'Rs&nbsp;<span id="original-price-id-' + productDTO.id + '">' + productDTO.price + '</span>';
					productHtml +=			'</strike>';
					productHtml +=			'<strong class="redText">&nbsp;Rs <span id="selling-price-id-' + productDTO.id + '">' + productDTO.voucherPrice + '</span></strong>';
					productHtml +=		'</div>';
					productHtml +=		'<div  class="product_list_view-discount '+(productDTO.discount > 0 ? '' : 'no-display')+' lfloat" id="discount-id-' + productDTO.id + '">' + productDTO.discount + '%</div>'; 
				}				
				productHtml += 		'</div>';
				if(productDTO.freebies){
					var freebieHtml = "";
					freebieHtml +=	'<div class="overhid">';
					freebieHtml +=		'<div class="lfloat">';
					freebieHtml +=			'<img class="freebie_img" src="'+ window.sdstaticPath +'img/freebie/freebie_image.png" />';
					freebieHtml +=		'</div>';
					freebieHtml +=		'<div class="lfloat freebie_text_wrapper">';
					var sizeCtr = 0;
					if(typeof productDTO.freebies != "string" && typeof productDTO.freebies != "number"){
						for(var i=0; i < productDTO.freebies.length; i++){
							if(productDTO.freebies[i]==""){
								continue;
							}
							freebieHtml +=		'<span class="freebie_text">'+productDTO.freebies[i]+'</span>';
							sizeCtr++;
							if(sizeCtr < productDTO.freebies.length){
								freebieHtml += 	'&nbsp;<img class="freebie_sign_img" src="'+ window.sdstaticPath +'img/freebie/plus_sign.png" />&nbsp;';
							}
						}
					}else{
						freebieHtml +=			'<span class="freebie_text">'+productDTO.freebies+'</span>';
					}
					freebieHtml += 		'</div>';
					freebieHtml +=	'</div>';
					if(typeof productDTO.freebies != "string" && productDTO.freebies.length == 0){
					}else{
						productHtml += freebieHtml;
					}
				}
				productHtml += 		'<div class="overhid" style="margin: 8px 0 11px;">';
				productHtml += 			'<div  style="margin: 8px 0 11px;" class="overhid">';
				if(!productDTO.prebook){
					productHtml +=			'<div class="lfloat product_list_view_shipped fnt-tahoma">Dispatched in ' + productDTO.shippingDays + ' business days</div>';
				}
				if(productDTO.codValid){
					if(!productDTO.prebook){
						productHtml +=		'<div class="lfloat product_list_view_cod fnt-tahoma">Cash on Delivery Available</div>';
					}else{
						productHtml +=		'<div class="lfloat no-border product_list_view_cod fnt-tahoma">Cash on Delivery Available</div>';
					}
				}
				if(productDTO.voucherPrice > emiThreshold){
   					if(!productDTO.codValid && productDTO.prebook){
						productHtml +=		'<div class="lfloat no-border product_list_view_emi fnt-tahoma">&nbsp;&nbsp;Interest Free EMI</div>';
   					}else{
   						productHtml +=		'<div class="lfloat product_list_view_emi fnt-tahoma">&nbsp;&nbsp;Interest Free EMI</div>';
   					}
				}
				productHtml += 			'</div>';
				if(productDTO.initAttributesFull){
					productHtml += 		'<div class="prod-buy-outer">';
					productHtml += 			'<div class="product-attr-outer">';
					productHtml += 				'<div id="attribute-error-message-'+productDTO.id+'" style="display: none" class="error">Please select attribute(s)</div>';
					productHtml += 				'<div style="display:none;" id="productAttributesJson-'+productDTO.id+'">';
					productHtml	+= 					JSON.stringify(productDTO.initAttributesFull);
					productHtml += 				'</div>';
					productHtml += 				'<ul id="product-option-'+productDTO.id+'" class="product-attr prod-'+productDTO.id+'">';
					productHtml +=					'<script type="text/javascript">createAttributes("'+productDTO.id+'","true")<\/script>';
					productHtml += 				'</ul>';
					productHtml += 			'</div>';
					productHtml += 		'</div>';					
				}
				productHtml += 			'<div class="overhid product_list_view_buy-outer">';
				if(productDTO.live){
					if(!productDTO.prebook && !productDTO.soldOut){
						productHtml +=		'<div class="lfloat product_list_view-buyBtn">';
						productHtml +=			'<a href="" class="buylink addToCart prodlistBuy-'+productDTO.id+' liveBuy" catalog="'+((productDTO.initAttributesFull)?'undefined':productDTO.offers.id)+'" prebook="true" id="listBuy-'+productDTO.id+'" onclick="return onBuyClick('+productDTO.id+');">Buy</a>';
						productHtml +=		'</div>';
					}else if(productDTO.prebook && !productDTO.soldOut){
						productHtml +=		'<div class="lfloat product_list_view-preorderBtn">';
						productHtml +=			'<a href="" class="buylink addToCart prodlistBuy-'+productDTO.id+' liveBuy" catalog="'+((productDTO.initAttributesFull)?'undefined':productDTO.offers.id)+'" prebook="true" id="listBuy-'+productDTO.id+'" onclick="return onBuyClick('+productDTO.id+');">Pre-Order</a>';
						productHtml +=		'</div>';
					}else{
						productHtml +=		'<div class="lfloat list-view-soldOut redText">SOLD OUT</div>';		
					}
				}else{
					productHtml +=			'<div class="lfloat list-view-soldOut redText">OOPS YOU ARE LATE</div>';
				}
				productHtml +=				'<div class="lfloat product_list_view_viewLink">';
				productHtml +=					'<a class="hit-ss-logger" v="p" categoryId="'+$("#categoryId").val()+'" href="'+window.sdHttpPath+'/'+productDTO.pageUrl + '?pos=' + absPos + ';' + numFound + '" title="'+ productDTO.vendorName + '" pogId="'+productDTO.id+'">View Details</a>';
				productHtml +=				'</div>';
				productHtml += 			'</div>';
				productHtml += 		'</div>';
				productHtml += 	'</div>';
				if(productDTO.highlights){
					var highHtml = "";
					highHtml += '<div class="rfloat product_list_view_highlights">';
					highHtml += 	'<div class="product_list_view_heading">Highlights</div>';
					highHtml += 	'<div class="key-features-outer">';
					highHtml += 		'<ul class="key-features">';
					if(productDTO.highlights.length !=undefined){
						var runLimit = (productDTO.highlights.length > 4 ? 4 : productDTO.highlights.length);
						for(var i = 0; i<runLimit; i++){
							highHtml +=		'<li>'+trimHighlights(productDTO.highlights[i].highlight)+'</li>';
						}
					}else{
						highHtml += 		'<li>'+trimHighlights(productDTO.highlights.highlight)+'</li>';
					}
					highHtml +=      	'</ul>';
					highHtml += 	'</div>';
					highHtml += '</div>';
					if(productDTO.highlights.length != undefined && productDTO.highlights.length == 0){
					}else{
						productHtml += highHtml;
					}
				}
				productHtml +='</div>';
				if( !( $.browser.msie && $.browser.version == '8.0' ) ) {
                     productHtml +=         '</div>';
             	}
				return productHtml;
			}
			function trimHighlights(highlight){
				var maxLengthChars = 55;
				var highRendered = (highlight.length > maxLengthChars) ? highlight.substring(0, maxLengthChars-3)+'...' : highlight;	
				return highRendered;
			}
			function prebookHelpClick(productId){
				$('#prebookInfo-'+productId).show();
			}
			function prebookHelpLeave(productId){
				$('#prebookInfo-'+productId).delay(100).hide(0);
			}
			function getRetargetingScript(eventType,categoryId) {				var ajaxUrl = "/getretargetingscript?eventType=" + eventType + "&categoryId=" + categoryId ;
				$.ajax({
					url : ajaxUrl,
					type : "GET",
					dataType : 'html',
					success : function(data) {
						$('body').append(data);
					},
					error: function(){
						$('#ajax-loader-icon').hide();	
					}
				});
			}
			$(document).ready(function() {
				Snapdeal.Cookie.set('pc', '364', 90);
				window.cachePageData = new Snapdeal.CachePageData();
				window.cachePageData.init();
				comparedProdGetData();
				removeProductLastBor();
				if($("#zoneId").val() != 0){
					var zonePageUrl = $("#zoneselectautocomplete").attr("pageUrl");
					if(zonePageUrl !== undefined && zonePageUrl.length > 0) {
						Snapdeal.Cookie.set('zonePageUrl', zonePageUrl, 10000);	
					}
					$('.skipit').attr("href", window.location.href);
				};
				$('#partialSearchKey').html('"'+$('#keyword').val().split(' ').toString().replace(/\,/g,'", "')+'"');
				$('#cityul').click(function(){
					if($("#zoneId").val() != 0){
						var zonePageUrl = $("#zoneselectautocomplete").attr("pageUrl");
						if(zonePageUrl !== undefined && zonePageUrl.length > 0) {
							Snapdeal.Cookie.set('zonePageUrl', zonePageUrl, 10000);	
						}
						$('.skipit').attr("href", window.location.href);
					}
				});
				$("#skipmail").click(function(){
                    setCookie("s","1", 7, "/", ".snapdeal.com");
                });
				$(".filter-value").click(function() {
					$(this).parent().toggleClass("active-filter");
				});
				$('.reset-filter').click(function() {
					$("#filter-no-results-message").hide();
					var filterType = $(this).attr('filterType');
					var filterName = $(this).attr('filterName');
					if(filterType == 'checkbox') {
						$("input:checkbox[name="+filterName+"]").attr('checked', false);
						$("input:checkbox[name="+filterName+"]").parent('label').removeClass('active-filter');
						changeFilters();
					} else if(filterType == 'radio') {
						$("input:radio[name="+filterName+"]").attr('checked', false);
						changeFilters();
					} else if(filterType == 'slider') {
						var min = parseInt($(this).attr('min'));
						var max = parseInt($(this).attr('max'));
						$('#slider-'+filterName+'-id').slider('option', 'values', [min, max]);
						//we will not call chane filter here beacuse on changing the values it will automatically calls change method of the slider.
					}					
				});
				$('.sd-slider').each(function() {
					var context = this;
					var smin = parseInt($(this).attr('min'));
					var smax = parseInt($(this).attr('max'));
					var pmin = smin;
					var pmax = smax;
					var strat = parseInt($(this).attr('startSelected'));
					var ennd = parseInt($(this).attr('endSelected'));
					if( $.browser.version == '8.0' && $.browser.msie ) {
						if( $(this).attr('interval') == "" ) $(this).attr('interval', 0);
						if( smin == smax ) {smax++;}
						if( strat == ennd ) {ennd++;}
					}
					$(this).slider({ 			
						range: true,
						min: smin,
						max: smax,
						values : [strat,ennd],
						step: parseInt($(this).attr('interval')),
						slide:function(event, ui) {
							var filterName = $(context).attr('name');
							$('#select-min-value-'+filterName).html(ui.values[ 0 ]);
							$('#select-max-value-'+filterName).html(ui.values[ 1 ]);
							$("#productResultsFound").hide();
							onFilter = true;
						},
						change: function( event, ui ) {
							if(pmin == ui.values[ 0 ] && pmax == ui.values[ 1 ]) {
								// do nothing
							} else {
								var filterName = $(context).attr('name');
								$("input[name="+filterName+"-slider]").val(ui.values[ 0 ] + "," + ui.values[ 1 ]);
								$('#select-min-value-'+filterName).html(ui.values[ 0 ]);
								$('#select-max-value-'+filterName).html(ui.values[ 1 ]);
								pmin = 	ui.values[ 0 ];
								pmax = ui.values[ 1 ];
								changeFilters();
								updateOmniture('changedFilter','FID', 'Price Filter: Rs'+pmin+ ' to '+pmax);
							}
						}
					});
				});
				$(".sort-by-box").click(function() {
					$(".sort-by-drop").toggle();
				});
				$('.sort-value').click(function(){
					$('.sort-by-box').html('<span>'+$(this).html()+'</span>');
					$(".sort-by-drop").toggle();
					$('#seeMoreProducts').attr('sortType', $(this).attr('sortType'));
					changeFilters(); 
					updateOmniture('changedFilter','FID', 'Sort Filter: '+ $(this).html());
					$('.sort-value').each( function() {
						if( $(this).html() == $('.sort-by-box span').html() ) { $(this).hide(); }  
						else { $(this).show(); }
					});	
				});
				var mylist = $('#categoryDropdown');
				var listitems = mylist.children('li').get();
				listitems.sort(function(a, b) {
				   var compB = parseInt($(a).attr('count'));
				   var compA = parseInt($(b).attr('count'));
				   return (compA < compB) ? -1 : (compA > compB) ? 1 : 0;
				})
				$.each(listitems, function(idx, itm) { mylist.append(itm); });
				window.sdstaticPath = Snapdeal.getResourcePath("");
				window.sdHttpPath = $('#sd-http-path').html();
				window.zonePageUrl = $('#zone-page-url').html();
				/*
				$('.product-container li:odd').addClass('right');
				$('.product-container li:even').addClass('left');
				*/
				$('.category-select a').click(function() {
					$('.category-box').slideToggle();
					$('.category-select').parent().addClass("product_more_selected");
					$('.category-select span a').addClass("selected-arrow");
				});
				$(document).mouseup(function() {
					$(".category-select").parent().removeClass('product_more_selected');
					$('.category-select span a').removeClass("selected-arrow");
					$(".category-box").hide();
				});
				$('.list').click(function(){
					if(!$('.list').parent('.lfloat.view_type').hasClass('inactive_view')){
						return false;
					}
					//$("#products-main4").addClass("white-overlay");
					$("#filter-no-results-message").hide();
					var categoryId = $('#seeMoreProducts').attr('categoryid');
					Snapdeal.Cookie.set("vT", categoryId + "|List", 10000);
					$('#selectedViewType').html('List');
					selectedView = 'List';
					$('.lfloat.view_type').toggleClass('inactive_view');
					$('#seeMoreProducts').attr('start',0).attr('end',5);
					launchOmnScript();
					getProducts($('#seeMoreProducts').attr('categoryid'), 0, 5, false, encodeURIComponent($('#seeMoreProducts').attr('qparam')), $('#seeMoreProducts').attr('sortType'),"List");
					//$("#products-main4").removeClass("white-overlay");
				});
				$('.grid').click(function(){
					if(!$('.grid').parent('.lfloat.view_type').hasClass('inactive_view')){
						return false;
					}
					//$("#products-main4").addClass("white-overlay");					
					$("#filter-no-results-message").hide();
					var categoryId = $('#seeMoreProducts').attr('categoryid');
					Snapdeal.Cookie.set("vT", categoryId + "|Grid", 10000);
					$('#selectedViewType').html('Grid');
					selectedView = 'Grid';
					$('.lfloat.view_type').toggleClass('inactive_view');
					$('#seeMoreProducts').attr('start',0).attr('end',20);
					launchOmnScript();
					getProducts($('#seeMoreProducts').attr('categoryid'), 0, 20, false, encodeURIComponent($('#seeMoreProducts').attr('qparam')), $('#seeMoreProducts').attr('sortType'),"Grid");
					//$("#products-main4").removeClass("white-overlay");
				});
			});
			function onBuyClick(buyBtnId){
				var buyId = buyBtnId;
                if ($('#listBuy-'+buyId).attr('href').indexOf("catalog=undefined") != -1) {
                    $('#attribute-error-message-'+buyId).show();
                    return false;
                }
                return true;
			}
			$('#seeMoreProducts').click(function() {
				noSpin = true;
				$('#seemore').hide();
				$('#seeMoreState').val('hidden');
				$('#ajax-loader-icon').show();
				//productInViewRange(parseInt($(this).attr('start')) + parseInt(20),406);
				if(selectedView=="List"){
					getProducts($(this).attr('categoryid'), $(this).attr('start'), 5, true, encodeURIComponent($(this).attr('qparam')), $(this).attr('sortType'),selectedView);
				}else {
					getProducts($(this).attr('categoryid'), $(this).attr('start'), 20, true, encodeURIComponent($(this).attr('qparam')), $(this).attr('sortType'),selectedView);
				}
			});
			$(window).scroll(function(){
				if($('#seemore').is(":visible")){
					if ($(window).scrollTop() >= $(document).height() - $(window).height() -1610){
						noSpin = true;
			        	$('#seemore').hide();
			        	$('#seeMoreState').val('hidden');
						$('#ajax-loader-icon').show();
						if(selectedView=="List"){
							getProducts($('#seeMoreProducts').attr('categoryid'), $('#seeMoreProducts').attr('start'), 5, true, encodeURIComponent($('#seeMoreProducts').attr('qparam')), $('#seeMoreProducts').attr('sortType'), selectedView);
						}else {
							getProducts($('#seeMoreProducts').attr('categoryid'), $('#seeMoreProducts').attr('start'), 20, true, encodeURIComponent($('#seeMoreProducts').attr('qparam')), $('#seeMoreProducts').attr('sortType'), selectedView);
						}
			        }	
				}
			}); 
			function productInViewRange(showingCount, totalCount)
			{
				var productCount = " Showing " + showingCount + " of " + totalCount;
				$('#productViewRange').html(productCount);
			}			
			function updateOmniture(eventType, param, value) {
				var ajaxUrl = "/omn/getOmnitureCode?eventType=" + eventType + "&param=" + param + "&value=" + value;
				$.ajax({
					url : ajaxUrl,
					type : "POST",
					contentType : 'application/json; charset=utf-8',
					dataType : 'html',
					success : function(data) {
						$('body').append(data);
					},
					error : function() {
						$('#ajax-loader-icon').hide();
					}
				});
			}
			function launchOmnScript(){
				var paramMap = {};
				paramMap["viewType"] = $("#selectedViewType").html();
				paramMap["viewTypeStatus"] = ($("#selectedViewType").html() == $("#defaultViewType").html())?'default':'changed';
				paramMap["categoryId"] = $("#seeMoreProducts").attr('categoryId');
				var isFromSearch = $("#isFromSearch").html();
				if(isFromSearch == "true"){
					paramMap["clcksrc"] = clickSrcValue;
					paramMap["resultsOnPage"] = $("#resultsOnPage").val();
					fireOmnitureScript(paramMap,"searchResults","/products/getomniturescript/listgridview");
				}else{
					fireOmnitureScript(paramMap,"listGridView","/products/getomniturescript/listgridview");
				}
				return;
			}
			//params parameter expects array of values like defined here : http://api.jquery.com/jQuery.ajax/ 
			//See "launchOmnScript" for sample
			function fireOmnitureScript(params, scriptName, hitUrl){
				params["name"] = scriptName;
				$.ajax({
					url : hitUrl,
					type : "POST",
					data : $.param(params),
					dataType : "html",
					success : function(data) {
						$('body').append(data);
					},
					error: function(){
						$('#ajax-loader-icon').hide();
					}
				});
			}
		</script>
		<script>
		Snapdeal.CachePageData = function() {
			var self = this;
			this.init = function(){
				if($('#pageReloaded').val() == 'true'){
					self.reloadPageFromCache();
				}else{
						if ($('#catName').val() != "") {
							var dropdownSelected = '';
							if (dropdownSelected == 'true') {
								var productname = $('#catName').val();
								if (productname.length > 10) {
									pName = productname.substring(0, 9) + "...";
								} else {
									pName = productname;
								}
								$(".category-select a").text(pName);
							}
							$(".category-select").removeClass('selected');
							if(selectedView=="List"){
								$('#seeMoreProducts').attr('categoryid',$('#catId').val()).attr('start', 5);
								$('#startProductState').val(5);
							}else{
								$('#seeMoreProducts').attr('categoryid',$('#catId').val()).attr('start', 20);
								$('#startProductState').val(20);
							}
						} else {
							$(".category-select a").text('More');
							$(".category-select").removeClass('selected');
						}
						$('#pageReloaded').val('true');
						if(selectedView=="List"){
							var productCount = $('.product_list_view_cont').length;
							var prodNum = 5;
						}else{
							var productCount = $('.product_grid_cont').length;
							var prodNum = 20;
						}
						if (productCount < prodNum) {
							$('#seemore').hide();
							$('#seeMoreState').val('hidden');
						} else {
							$('#seemore').show();
							$('#seeMoreState').val('visible');
						}
					}
					$(".category-box").hide();
					$('.products_wrapper').show();
				}
				this.updateProductDto = function(data, start, type) {
					var existingProductDtos = $('#productDTO').val();
					if(type=="Grid"){
						if (start == 0 || existingProductDtos == '') {
							$('#productDTO')
									.val(
											JSON
													.stringify(data.minProductResponseDTO.productDtos));
						} else {
							existingProductDtos = JSON.parse(existingProductDtos);
							if (data.minProductResponseDTO.status == 'Success') {
								var productDtos = data.minProductResponseDTO.productDtos;
								existingProductDtos = existingProductDtos
										.concat(productDtos);
								$('#productDTO').val(
										JSON.stringify(existingProductDtos));
							}
						}
					}else{
						if (start == 0 || existingProductDtos == '') {
							$('#productDTO').val(JSON.stringify(data.listViewProductResponseDTO.productOfferGroupDtos));
						} else {
							existingProductDtos = JSON.parse(existingProductDtos);
							if (data.listViewProductResponseDTO.status == 'Success') {
								var productOfferGroupDtos = data.listViewProductResponseDTO.productOfferGroupDtos;
								existingProductDtos = existingProductDtos.concat(productOfferGroupDtos);
								$('#productDTO').val(JSON.stringify(existingProductDtos));
							}
						}
					}
				}
				this.reloadPageFromCache = function() {
					window.sdstaticPath = Snapdeal.getResourcePath("");
					window.sdHttpPath = $('#sd-http-path').html();
					window.zonePageUrl = $('#zone-page-url').html();
					var seeMoreState = $('#seeMoreState').val();
					if (seeMoreState == 'visible') {
						$('#seemore').show();
					} else {
						$('#seemore').hide();
					}
					var startProductState = $('#startProductState').val();
					$('#seeMoreProducts').attr('start', startProductState);
					$('#seeMoreProducts').attr('categoryid', $('#catId').val());
					var existingProductDtos = $('#productDTO').val();
					existingProductDtos = JSON.parse(existingProductDtos);
					var data = {};
					var selectedViewType = $('#selectedViewType').html();
					var categId = $('#catId').val();
					var viewTypeCookie = Snapdeal.Cookie.get("vT");
					if(viewTypeCookie != null){
						var viewTypeArray = viewTypeCookie.split("|");
						if(categId == viewTypeArray[0]){
							selectedViewType = viewTypeArray[1];
							selectedView = selectedViewType;
							$('#selectedViewType').html(selectedViewType);
							if(selectedViewType == "Grid"){
								$('.grid').parent().removeClass();
								$('.grid').parent().addClass("lfloat view_type");
								$('.list').parent().removeClass();
								$('.list').parent().addClass("lfloat view_type inactive_view");
							}
							if(selectedViewType == "List"){
								$('.grid').parent().removeClass();
								$('.grid').parent().addClass("lfloat view_type inactive_view");
								$('.list').parent().removeClass();
								$('.list').parent().addClass("lfloat view_type");
							}
						}
					}
					if(selectedViewType == "Grid"){
						data.minProductResponseDTO = {};
						data.minProductResponseDTO.status = 'Success';
						data.minProductResponseDTO.productDtos = existingProductDtos;
						data.minProductResponseDTO.productDtos.length = existingProductDtos.length;
						self.restoreHtml(data, selectedViewType);
					}else if(selectedViewType == "List"){
						data.listViewProductResponseDTO = {};
						data.listViewProductResponseDTO.status = 'Success';
						data.listViewProductResponseDTO.productOfferGroupDtos = existingProductDtos;
						data.listViewProductResponseDTO.productOfferGroupDtos.length = existingProductDtos.length;
						self.restoreHtml(data, selectedViewType);
					}
				}
				this.restoreHtml = function(data, selectedViewType) {
					var productHtml = "";
					var noOfProducts = 0;
					if(selectedViewType == "Grid"){
						if (data.minProductResponseDTO.productDtos.length == undefined) {
							productHtml += self.getProductHtml(data.minProductResponseDTO.productDtos, 1, 1);
							noOfProducts = 1;
						} else {
							noOfProducts = data.minProductResponseDTO.productDtos.length;
							for ( var i = 0; i < data.minProductResponseDTO.productDtos.length; i++) {
								productHtml += self.getProductHtml(data.minProductResponseDTO.productDtos[i], i, noOfProducts);
							}
						}
					}else if(selectedViewType == "List"){
						if (data.listViewProductResponseDTO.productOfferGroupDtos.length == undefined) {
							productHtml += getListProductHtml(data.listViewProductResponseDTO.productOfferGroupDtos, 1,noOfProducts, 0);
							noOfProducts = 1;
						} else {
							noOfProducts = data.listViewProductResponseDTO.productOfferGroupDtos.length;
							for ( var i = 0; i < noOfProducts; i++) {
								productHtml += getListProductHtml(data.listViewProductResponseDTO.productOfferGroupDtos[i], i, noOfProducts, 0);
							}
						}
					}
					$(".products_wrapper").html(productHtml);
					$('#ajax-loader-icon').hide();
				}
				this.getProductHtml = function(productDTO, index) {
					var productHtml = '';
				    var brRite = '';
					if(index == 0 || (index+1)%4 == 1) {
						productHtml += '<div class="grid_row_height product_grid_row">';
					}
					productHtml += '<div class="product_grid_cont" id="'+productDTO.id+'" ><a class="hit-ss-logger" v="p" pogId="'+productDTO.id+'" categoryId="'+$("#categoryId").val()+'" href="'+window.sdHttpPath+'/'+productDTO.pageUrl+'" title="'+ productDTO.vendorName + '">';
					productHtml +='<div class="product-image">' 
					+ '<img src="'+ window.sdstaticPath + productDTO.image +'" width="166" height="194" border="0" alt="'+ productDTO.vendorName +'" />'
					+ ((productDTO.soldOut) == true ?'<div class="soldout"></div>':'')
					+ '</div>'
					+ '<div class="product_grid_cont_heading">' + productDTO.vendorName + '</div>';
					if(productDTO.noOfReviews > 0 && reviewFlag == true){
						if(productDTO.noOfReviews > 1){
						productHtml +='<div class="overhid productReviews"><span class="stars lfloat"><span style="width:'+Math.max(0, (Math.min(5, parseFloat(productDTO.avgRating)))) * 16+'px;"/></span><span class="lfloat" style="font-size:11px; padding-left:5px;color:#727272;">| '+productDTO.noOfReviews+' Reviews</span></div>';
						}
						if(productDTO.noOfReviews == 1){
						productHtml +='<div class="overhid productReviews"><span class="stars lfloat"><span style="width:'+Math.max(0, (Math.min(5, parseFloat(productDTO.avgRating)))) * 16+'px;"/></span><span class="lfloat" style="font-size:11px; padding-left:5px;color:#727272;">| '+productDTO.noOfReviews+' Review</span></div>';
						}
					}					
					if(productDTO.freebies!=null &&  productDTO.freebies.length>0){
						productHtml += '<div class="freebies_inside" align="center"><img src="'+window.sdstaticPath+'img/freebie/freebie_inside.png?v=1"></div>';
					}
					productHtml += '<div class="product_grid_cont_price_outer">' ;
					if (productDTO.price == null && productDTO.voucherPrice == null){
						productHtml+='<div class="fnt12 prod-prebook">Pre Order at <strong class="blackText">Rs ' + productDTO.displayPrice + '</strong></div>';
					}else{
						productHtml+= '<div class="product_price ' + (productDTO.discount > 0 ? '' : 'no-float') + '"><span class="originalprice ' + (productDTO.discount > 0 ? '' : 'no-display') + '">Rs&nbsp;' + productDTO.price + '</span>  Rs&nbsp;' + productDTO.displayPrice + '</div>' ;
					}
					if(productDTO.discount>0){
						productHtml += '<div class="product_discount_outer"><div class="product_discount">' + productDTO.discount + '% <span>Off</span></div></div>';
					}
					productHtml += '</div>';
					productHtml += ((productDTO.codValid) == true ?'<div class="product-cod">Cash on delivery available</div>':'')
					+ '</a>';
					var attrs = Snapdeal.Utils.fixArray(productDTO.initAttributes); 
					if(productDTO.initAttributes != null && attrs.length > 0) {
						productHtml += '<span class="productAttr"> Select '
						+ attrs[0].name
						+ '<select onChange=" redirectToPDP( this, \' ' + window.sdHttpPath+'/'+productDTO.pageUrl + '\')">'
						+ '<option value="">--</option>';
						for( i=0; i<attrs.length; i++ ) {
							if( attrs[0].name == attrs[i].name ){
								if(!attrs[i].soldOut && attrs[i].live){
									productHtml += '<option value="' + attrs[i].value + '">' + attrs[i].value + '</option>';
								}
							}
						}
						productHtml += '</select>'
						+ '</span>';
					}
					if(compareFlag=='true'){
						productHtml += '<label class="addToCompare overhid"><input type="checkbox" id="checkboxAddCompare-'+productDTO.id+'" class="lfloat" onClick="addToCompareClick(this)"/> <div class="lfloat" style="padding:2px 0 0 1px">Add to compare</div></label>';
					}
					productHtml += '</div>';
					productHtml += ( (index+1)%4 == 0 ) ? '</div>' : ''; 
					return productHtml;
				}
			}		
		</script>
		<script>
		$(document).ready(function(){
			if($('#defaultViewType').html()=='List'){
						var idList = 0;
				var idListLen = idList.length;
				for(var counter=0; counter<idListLen; counter++){
					createAttributes(idList[counter],"false");
				}
			}
		});
		function createAttributes(productOGId, calledThroughAJAX){
			if(calledThroughAJAX=="true"){
				var attributes = $("#productAttributesJson-"+productOGId).text();
			}else{
				var attributes = $("#productAttributesJson-"+productOGId).val();
			}
			if(attributes && attributes == ""){
				return;
			}
            if (attributes && attributes != "undefined") {
       	 		var attributeJson = JSON.parse(attributes);
                window.attributeHandler = new Snapdeal.AttributeHandler(productOGId, calledThroughAJAX);
                window.attributeHandler.init(attributeJson);
            }
		}
		Snapdeal.AttributeHandler = function(pogId, calledThroughAJAX) {
            var self = this;
            this.attributeNames = {};
            this.init = function(attributeJson) {
                this.attributeJson = attributeJson;
                var depth = this.getDepth();
                var selectHtml = "";
                for ( var i = 0; i < depth; i++) {
                    selectHtml +='<li id="attr-level-' + i + '-'+ pogId+'">';
                    selectHtml += 	'<div class="opti-txt lfloat"> Select ' + this.attributeNames[i] + '</div>';
                    selectHtml += 	'<div class="lfloat">';
                    selectHtml += 		'<select class="selmenu attributes prod-' + pogId + ' product-home-attributes disabled" level="'+ i + '" id="attribute-select-' + i +'-'+ pogId + '" autocomplete="off" disabled="true">';
                    selectHtml +=			'<option value="">--</option>';
                    selectHtml += 		'</select>';
                	selectHtml +=	'</div>';
                	selectHtml +='</li>';
                }
                $("#product-option"+'-'+ pogId).html(selectHtml);
                var level1Html = "<option value=''>--</option>";
                if(this.attributeJson.subAttributes == undefined){
                	if(depth == 1){
                		if(this.attributeJson.length == undefined){
                			if(!this.attributeJson.live){
                				$("#product-option"+'-'+ pogId).html("");
                				return;
                			}else{
                				var soldOutMessage = "";
                				var disabled = "";
                				if(depth == 1 && this.attributeJson.soldOut) {
                                    soldOutMessage = " - Sold Out";
                                    disabled = 'disabled="true"';
                                }
            					level1Html += "<option value='" + this.attributeJson.value + "'" + disabled + ">" + this.attributeJson.value + soldOutMessage + "</option>";
                			}
                		}
                	}
	                for ( var i = 0; i < this.attributeJson.length; i++) {
	                	if(depth == 1 && !this.attributeJson[i].live) {
	                        continue;
	                    }
	                	var soldOutMessage = "";
	                    var disabled = "";
	                    if(depth == 2) {
	                    	var subAttributes = this.attributeJson[i].subAttributes;
	                    	var showFlag = false;
	                    	if(subAttributes != undefined){
		                    	if(subAttributes.length != undefined){
			                        for ( var j = 0; j < subAttributes.length; j++) {
			                            if(subAttributes[j].live){
			                            	showFlag = true;
			                            	break;
			                            }
			                        }
		                    	}else{
		                    		if(subAttributes.live){
		                    			showFlag = true;
		                    		}
		                    	}
	                    	}else{
	                    		showFlag = false;
	                    	}
	                        if(showFlag == false) {
	                        	disabled = 'disabled="true"';
	                        }
	                    }
	                    if(depth == 1 && this.attributeJson[i].soldOut) {
	                        soldOutMessage = " - Sold Out";
	                        disabled = 'disabled="true"';
	                    }
						level1Html += "<option value='" + this.attributeJson[i].value + "'" + disabled + ">" + this.attributeJson[i].value + soldOutMessage + "</option>";
	                }
                }else if(depth == 1 && !this.attributeJson.live){
						level1Html += "";
				}else{
                	var soldOutMessage = "";
                    var disabled = "";
                    if(depth == 2) {
                    	var subAttributes = this.attributeJson.subAttributes;
                    	var showFlag = false;
                    	if(subAttributes.length != undefined){
	                        for ( var j = 0; j < subAttributes.length; j++) {
	                            if(subAttributes[j].live){
	                            	showFlag = true;
	                            	break;
	                            }
	                        }
	                        if(showFlag == false) {
	                        	disabled = 'disabled="true"';
	                        }
                    	}else{
                    		if(subAttributes.live){
                            	showFlag = true;
                            }
                    	}
                    }
                    if(depth == 1 && this.attributeJson.soldOut) {
                        soldOutMessage = " - Sold Out";
                        disabled = 'disabled="true"';
                    }
					level1Html += "<option value='" + this.attributeJson.value + "'" + disabled + ">" + this.attributeJson.value + soldOutMessage + "</option>";
                }
                $("#attribute-select-0"+'-'+ pogId).attr('disabled', false).removeClass('disabled');
                $("#attribute-select-0"+'-'+ pogId).html(level1Html);
                $(".attributes.prod-" + pogId).change(function() {
                    self.attributeChange($(this));
                });
                if( $("#product-option"+"-"+ pogId + " li:last") != null ) {
                	$("#product-option"+"-"+ pogId + " li:last").children().eq(1).children().attr('disabled', false);
                	self.attributeChange($("#product-option"+"-"+ pogId + " li:first select"));
                }
			};
            this.getDepth = function() {
                var depth = 0;
                var attributeNames = {};
                var subAttributes = this.attributeJson;
                var test = "array";
                if(subAttributes.length == undefined){
                	test = "object";
                }else{
                	test = "array";
                }
                while(true){
                	if(test=="object"){
                		this.attributeNames[depth] = subAttributes.name;
                		depth++;
                		subAttributes = subAttributes.subAttributes;
                		if(subAttributes == undefined || subAttributes == null || subAttributes == ""){
                			break;
                		}
                	}else{
                		this.attributeNames[depth] = subAttributes[0].name;
                		depth++;
                		subAttributes = subAttributes[0].subAttributes;
                		if(subAttributes == undefined || subAttributes == null || subAttributes == ""){
                			break;
                		}
                	}
                	if(subAttributes.length == undefined){
                    	test = "object";
                    }else{
                    	test = "array";
                    }
                }
                this.depth = depth;
                return depth;
            };
            this.attributeChange = function(aSelect) {
                var level = parseInt(aSelect.attr("level"));
                var buyLink = Snapdeal.getStaticPath('') + "/checkout?catalog=";
                var value = aSelect.val();
                var catalogId = 'undefined';
                if (value === "") {
                    // disable rest of select options
                    for ( var i = level + 1; i < this.depth; i++) {
                        $("#attribute-select-" + i + "-"+ pogId).html("<option value=''>--</option>");
                        $("#attribute-select-" + i + "-"+ pogId).attr('disabled', true).addClass('disabled');
                    }
                    buyLink += "undefined";
                } else {
                    if (level != this.depth - 1) {
                        var optionHtml = "";
                        var subAttributes = this.getAttributesAtDepth(level + 1);
                        var levelHtml = "<option value=''>--</option>";
                        if(subAttributes!=undefined){
	                        if(subAttributes.length != undefined){
		                        for ( var i = 0; i < subAttributes.length; i++) {
		                            var soldOutMessage = ""
		                            var disabled = "";
		                            if(!subAttributes[i].live){
		                            	continue;
		                            }
		                            if(level == this.depth - 2 && subAttributes[i].soldOut) {
		                                soldOutMessage = " - Sold Out"; 
		                                disabled = 'disabled="true"';
		                            } 
		                            levelHtml += "<option value='" + subAttributes[i].value + "'"+ disabled +">" + subAttributes[i].value + soldOutMessage + "</option>";
		                        }
	                        }else{
	                        	var soldOutMessage = ""
		                        var disabled = "";
	                        	if(subAttributes == ""){
	                        		disabled = 'disabled="true"';
	                        		levelHtml += "<option value='--'"+ disabled +">--</option>";
	                        	}else if(!subAttributes.soldOut){
		                            levelHtml += "<option value='" + subAttributes.value + "'"+ disabled +">" + subAttributes.value + soldOutMessage + "</option>";
	                        	}else{
	                        		soldOutMessage = " - Sold Out"; 
	                                disabled = 'disabled="true"';
		                            levelHtml += "<option value='" + subAttributes.value + "'"+ disabled +">" + subAttributes.value + soldOutMessage + "</option>";
	                        	}
	                        }
                        }
                        $("#attribute-select-" + (level + 1) + "-"+ pogId).html(levelHtml);
                        $("#attribute-select-" + (level + 1) + "-"+ pogId).attr('disabled', false).removeClass('disabled');
                        // disable rest of select options
                        for ( var i = level + 2; i < this.depth; i++) {
                            $("#attribute-select-" + i + "-"+ pogId).html("<option value=''>--</option>");
                            $("#attribute-select-" + i + "-"+ pogId).attr('disabled', true).addClass('disabled');
                        }
                        buyLink += "undefined";
                    } else {
                        var selectedValue = $("#attribute-select-" + level + "-"+ pogId).val();
                        var subAttributes = this.getAttributesAtDepth(level);
                        if(subAttributes.length != undefined){
	                        for ( var j = 0; j < subAttributes.length; j++) {
	                            if (subAttributes[j].value == selectedValue) {
	                                buyLink += subAttributes[j].catalogId;
	                                catalogId = subAttributes[j].catalogId;
	                                $('#original-price-id-'+ pogId).html(subAttributes[j].price);
	                                $('#selling-price-id-'+ pogId).html(subAttributes[j].sellingPrice);
	                                $('#discount-id-'+ pogId).html(subAttributes[j].discount);
	                            }
	                        }
                        }else{
                        	buyLink += subAttributes.catalogId;
                            catalogId = subAttributes.catalogId;
                            $('#original-price-id-'+ pogId).html(subAttributes.price);
                            $('#selling-price-id-'+ pogId).html(subAttributes.sellingPrice);
                            $('#discount-id-'+ pogId).html(subAttributes.discount);
                        }
                    }
                }
                $('#attribute-error-message-'+ pogId).hide();
                $('.prodlistBuy-'+pogId).attr('href', buyLink);
                $('.prodlistBuy-'+pogId).attr('catalog', catalogId);
            };
            this.getAttributesAtDepth = function(depth) {
                var subAttributes = this.attributeJson;
                for ( var i = 0; i < depth; i++) {
                    var selectedValue = $("#attribute-select-" + i + "-"+ pogId).val();
                    if(subAttributes.length != undefined){
	                    for ( var j = 0; j < subAttributes.length; j++) {
	                        if (subAttributes[j].value == selectedValue) {
	                            subAttributes = subAttributes[j].subAttributes;
	                            break;
	                        }
	                    }
                    }else{
                    	subAttributes = subAttributes.subAttributes;
                    }
                }
                return subAttributes;
            };
        };
		</script>

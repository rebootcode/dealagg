<script language="javascript" type="text/javascript">
    $(document).ready(function() {
        $('.button-buy-small').click(function() {
            $('.popup_cart').css('visibility', 'hidden');
            $(this).next('div').css('visibility', 'visible');
        });
    });
    var rowId;
    function AddBasketItem(productID, vendorID, rowNumber) {
        rowId = rowNumber;
        SearchResultService.AddBasketItem(productID, 1, 1, vendorID, "0", AddBasketItem_Callback);
    }
    function AddBasketItem_Callback(res) {

        //In case out of stock
        if (res == '-1')
            location.href = document.location.href;

        if (res != '' & res.indexOf(",") != -1) {
            var totalQty;
            var totalNet;
            var elem = res.split(',');
            totalQty = elem[0];
            totalNet = elem[1];
            document.getElementById("divCartPopup" + rowId).innerHTML = CreatePopUp(totalQty, totalNet, rowId);
            document.getElementById("ctl00_lblTotalCartItems").innerHTML = totalQty;
            ///Create Shopping Cart popup
            SearchResultService.GetShoppingCartPopup(ShoppingCartPopup_Callback);
        }
    }
    function ShoppingCartPopup_Callback(res) {
        document.getElementById("ctl00_divShoppingPopup").innerHTML = res;
    }
</script>

//                    totalQty = totalQty + parseInt(qty);
//                    SearchResultService.UpdateBasketQty(aryBasketIDs[i], qty, aryMFactors[i], UpdateBasketQty_Callback);
//                    lblQtyMsg.innerHTML = '';
//                }
//            }
//            return true;
//        }
//        else
//            return false;
    }
  
    function ShoppingCartPopup_Callback(res) {
        document.getElementById("ctl00_divShoppingPopup").innerHTML = res;
        var lblmsg = document.getElementById("lblPopupMsg");
        lblmsg.innerHTML = "Quantity updated successfully.";
    }
    var checkQtyAvalability = true;
    var checkQtyCount = 0;
    function validateBasketQty() {
        var ProductIDs = document.getElementById("hdnProductID").value;
        var isValid = true;
        if (ProductIDs.indexOf(",") != -1) {
            var aryProducts = ProductIDs.split(",");
            for (var i = 0; i < aryProducts.length - 1; i++) {
                itemCount = aryProducts.length - 1;
                var qty = document.getElementById("txtQty" + (i + 1)).value;
                var lblQtyMsg = document.getElementById("lblQtyMsg" + (i + 1));
                if (!(/^[1-9]+[0-9]*$/.test(qty))) {
                    isValid = false;
                    lblQtyMsg.innerHTML = "Invalid";
                }
                else if (qty > 500) {
                    lblQtyMsg.innerHTML = "To order bulk quantity more then 500, please contact at <a href='mailto:contact@uRead.com'>contact@uRead.com</a>.";
                    isValid = false;
                    //SearchResultService.CheckStockAvailability(aryProducts[i], qty);
                }
                else {
                    lblQtyMsg.innerHTML = '';
                }
            }
        }
        if (isValid) {
            if (ProductIDs.indexOf(",") != -1) {
                var aryProducts = ProductIDs.split(",");
                isValid = false;
                checkQtyCount = 0;
                checkQtyAvalability = true;
                for (var i = 0; i < aryProducts.length - 1; i++) {
                    var qty = document.getElementById("txtQty" + (i + 1)).value;
                    SearchResultService.CheckStockAvailability(aryProducts[i], qty, i + 1, CheckStockAvailability_Callback);
                }
          }
            
        }
    }
    function CheckStockAvailability_Callback(res) {
        checkQtyCount++;
       
        if (res.indexOf(",") != -1) {
            var aryRes = res.split(",");
            var lblQtyMsg = document.getElementById("lblQtyMsg" + aryRes[0]);
            if (aryRes[1] != 'valid') {
                lblQtyMsg.innerHTML = aryRes[1];
                checkQtyAvalability = false;
            }
        }
        if (checkQtyCount == itemCount & checkQtyAvalability == true) {
            
            var BasketIDs = document.getElementById("hdnBasketID").value;
            var MFactors = document.getElementById("hdnMF").value;
            var ProductIDs = document.getElementById("hdnProductID").value;
            if (BasketIDs.indexOf(",") != -1) {
                var aryBasketIDs = BasketIDs.split(",");
                var aryMFactors = MFactors.split(",");
                var aryProducts = ProductIDs.split(",");
                itemCount = aryBasketIDs.length - 1;
                for (var i = 0; i < aryBasketIDs.length - 1; i++) {
                    var qty = document.getElementById("txtQty" + (i + 1)).value;
                    var lblQtyMsg = document.getElementById("lblQtyMsg" + (i + 1));
                    totalQty = totalQty + parseInt(qty);
                    SearchResultService.UpdateBasketQty(aryBasketIDs[i], qty, aryMFactors[i], UpdateBasketQty_Callback);
                    lblQtyMsg.innerHTML = '';
                }
            }
        }

    }

    function UpdateBasketQty_Callback() {
        countCallService++;
        if (countCallService == itemCount) {
            document.getElementById("ctl00_lblTotalCartItems").innerHTML = totalQty;
            SearchResultService.GetShoppingCartPopup(ShoppingCartPopup_Callback);
        }
    }
    function DeleteBasketItem(BasketDetailID) {
        SearchResultService.DeleteBasketItem(BasketDetailID, DeleteBasketItem_Callback);
    }
    function DeleteBasketItem_Callback() {
        SearchResultService.GetShoppingCartPopup(DeleteShoppingCartPopup_Callback);
    }
    function DeleteShoppingCartPopup_Callback(res) {
        document.getElementById("ctl00_divShoppingPopup").innerHTML = res;
        totalQty = 0;
        if (document.getElementById("hdnBasketID") != null) {
            var BasketIDs = document.getElementById("hdnBasketID").value;
            if (BasketIDs.indexOf(",") != -1) {
                var aryBasketIDs = BasketIDs.split(",");
                itemCount = aryBasketIDs.length - 1;
                for (var i = 0; i < aryBasketIDs.length - 1; i++) {
                    var qty = document.getElementById("txtQty" + (i + 1)).value;
                    totalQty = totalQty + parseInt(qty);
                }
            }
           
        }
        document.getElementById("ctl00_lblTotalCartItems").innerHTML = totalQty;
    }
    $(document).ready(function() {
        $('#page-body').hover(function() {
            $('#ctl00_divShoppingPopup').hide();
        });
    });
   
</script>

</html>

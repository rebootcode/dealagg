 <script language="javascript">
        function ChangePageSize() {
            var index = document.getElementById("ddlPages").selectedIndex;
            var pageSize = document.getElementById("ddlPages").options[index].value
            SearchResultService.SetPageSize(pageSize, callback, onerror);
        }
       function callback() {            
            if ("filter" != "author")
            {
                var locpath= "http://www.uread.com" + "/search-books/" + encodeURIComponent("dan-brown") + "/" + "filter" + "_" + "0" + "_0"; 
                location.href = locpath;
            }
            else
            {
                
                var locpath="http://www.uread.com" + "/author/" + encodeURIComponent("dan-brown") + "/" + "0" + "_0"; 
                  location.href = locpath;
            }
                  //document.location.href;
        }
        function onerror() {
      
            if ("filter" != "author")
            {
                 var locpath= "http://www.uread.com" + "/search-books/" + encodeURIComponent("dan-brown") + "/" + "filter" + "_" + "0" + "_0"; 
                  location.href = locpath;
            }
            else
            {
                 var locpath= "http://www.uread.com" + "/author/" + encodeURIComponent("dan-brown") + "/" + "0" + "_0"; 
                  location.href = locpath;
            }
        }
 </script>

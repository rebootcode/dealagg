<div id="ctl00_TopSearch1_plnTopSearch" onkeypress="javascript:return WebForm_FireDefaultButton(event, 'ctl00_TopSearch1_Button1')">
	
    <div class="search-box">
        <input name="ctl00$TopSearch1$txtSearch" type="text" id="ctl00_TopSearch1_txtSearch" class="search-text-box" autocomplete="off" onblur="DoWaterMarkOnBlur(this,'Search for Book, Author, Publisher or ISBN');" onkeyup="updatehidden(this);" value="Search for Book, Author, Publisher or ISBN" onfocus="DoWaterMarkOnFocus(this,'Search for Book, Author, Publisher or ISBN');" />
        
        <input type="submit" name="ctl00$TopSearch1$Button1" value=" " onclick="return checkSearchText();" id="ctl00_TopSearch1_Button1" class="button-search" />
        <input type="hidden" name="ctl00$TopSearch1$hdnSearch" id="ctl00_TopSearch1_hdnSearch" />

        <script type="text/javascript">
            function updatehidden(e) {
                document.getElementById('ctl00_TopSearch1_hdnSearch').value = e.value;
            }
            function BoldChildren() {
                var behavior = $find('AutoCompleteEx');
                var target = behavior.get_completionList();
                var prefix = behavior._currentPrefix.toLowerCase();
                var i;
                for (i = 0; i < target.childNodes.length; i++) {
                    var sValue = target.childNodes[i].innerHTML.toLowerCase();
                    if (sValue.indexOf(prefix) != -1) {
                        var fstr = target.childNodes[i].innerHTML.substring(0, sValue.indexOf(prefix));
                        var estr = target.childNodes[i].innerHTML.substring(fstr.length + prefix.length, target.childNodes[i].innerHTML.length);
                        var mstr = target.childNodes[i].innerHTML.substring(sValue.indexOf(prefix), fstr.length + prefix.length);
                        target.childNodes[i].innerHTML = fstr + '<B>' + mstr + '</B>' + estr;
                    }
                }
            }
            function GetcolorChange(sender, e) {
                HideImage();
                var customers = sender.get_completionList().childNodes;
                var searchText = sender.get_element().value;
                for (var i = 0; i < customers.length; i++) {
                    var customer = customers[i];
                    var customerValueArray = (customer._value).split("|");
                    var customerId = customerValueArray[0];
                    customerId = customerId.replace(searchText, '<b>' + searchText + '</b>');
                    customer.innerHTML = customerId;
                    customer.className = "ac_results1 " + customerValueArray[1];
                }
                BoldChildren();
            }
            function ShowImage() {
                document.getElementById('ctl00_TopSearch1_txtSearch').style.backgroundImage = 'url(images/common/progress.gif)';
                document.getElementById('ctl00_TopSearch1_txtSearch').style.backgroundRepeat = 'no-repeat';
                document.getElementById('ctl00_TopSearch1_txtSearch').style.backgroundPosition = 'right';
            }

            function HideImage() {
                document.getElementById('ctl00_TopSearch1_txtSearch').style.backgroundImage = 'none';
            }

            function checkSearchText() {
                if (document.getElementById("ctl00_TopSearch1_txtSearch").value == 'Search for Book, Author or ISBN' || document.getElementById("ctl00_TopSearch1_txtSearch").value == '')
                    return false;
                else
                    return true;
            }
            function AutoCompleteExtender_ItemSelected(source, eventArgs) {
                var customerValueArray = (eventArgs._value).split("|");
                var selval = customerValueArray[0];
                if (selval == 'View More Results') {
                    document.getElementById("ctl00_TopSearch1_txtSearch").value = document.getElementById('ctl00_TopSearch1_hdnSearch').value
                    selval = document.getElementById("ctl00_TopSearch1_hdnSearch").value
                }
                else {
                    document.getElementById("ctl00_TopSearch1_txtSearch").value = selval;

                }
                var isTitle = 0;
                var isAuthor = 0;
                var isViewMore = 0;
                var isPublisher = 0;
                if (selval.indexOf(" (Author)") != -1) {
                    isAuthor = 1;
                }
                if (selval.indexOf(" (Title)") != -1) {
                    isTitle = 1;
                }
                if (selval.indexOf(" (Publisher)") != -1) {
                    isPublisher = 1;
                }
                if (selval.indexOf("View More Results") != -1) {
                    isViewMore = 1;
                }

                var strtoPass = selval.replace(" (Author)", "").replace(" (Publisher)", "").replace(" (Title)", "");
               
                if (strtoPass.substring(strtoPass.length - 1, strtoPass.length) == ".")
                    strtoPass = strtoPass.substring(0, strtoPass.length - 1);  
                    
                strtoPass = strtoPass.replace(/\s+/g, "-");
                strtoPass = strtoPass.replace(/\:/g, "");
                strtoPass = strtoPass.replace(/\\/g, "-");
                strtoPass = strtoPass.replace("/", "-");

                //strtoPass= strtoPass.replace(/\?/g,"")
                //strtoPass= strtoPass.replace(/([\^\$\(\)\[\]\{\}\*\,\@\+\:\?\|\\])/g,"").toLowerCase()
                //strtoPass= strtoPass.replace(/--/g,"");
                strtoPass = strtoPass.replace("’", "-");
                strtoPass = strtoPass.replace("“", "-");
                strtoPass = strtoPass.replace("”", "-");
                strtoPass = strtoPass.replace("?", "-");
                strtoPass = strtoPass.replace(/"/g, "-");
               

                if (isAuthor == 1 || isTitle == 1) {
                    strtoPass = strtoPass.replace(/\#/g, "")
                }
             
                strtoPass = escape(strtoPass).toLowerCase()
                if (isAuthor == 1) {
                    path = "/author/" + strtoPass;
                    window.location.href = path;
                }
                else if (isTitle == 1) {
                    path = "/search-books/" + strtoPass + "/title_0_0";
                    window.location.href = path;
                }
                else if (isPublisher == 1) {
                    path = "/publisher/" + strtoPass;
                    window.location.href = path;
                }
                else {
                    path = "/search-books/" + strtoPass;
                    window.location.href = path;
                }
            }
        
        </script>

    </div>

</div>


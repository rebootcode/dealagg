<script language="javascript" type="text/javascript">

    function ToggleFeedbackForm() {
        if (document.getElementById("divfeedback").style.display == 'none') {
            document.getElementById("divfeedback").style.display = 'block';
        }
        else {
            document.getElementById("divfeedback").style.display = 'none'
            document.getElementById("ctl00_phBody_SearchResult_SearchFeedback_txtName").value = '';
            document.getElementById("ctl00_phBody_SearchResult_SearchFeedback_txtPhone").value = '';
            document.getElementById("ctl00_phBody_SearchResult_SearchFeedback_txtEmail").value = '';
            document.getElementById("ctl00_phBody_SearchResult_SearchFeedback_txtDescription").value = '';
            document.getElementById("lblmsgSFB").style.display = 'none';
          
            
        }
    }
    function AddSearchFeedback() {
        
        if (Page_ClientValidate("valContactUs")) {
            document.getElementById('btnAdd').disabled = true;
            SearchResultService.AddSearchSuggestion(document.getElementById("ctl00_phBody_SearchResult_SearchFeedback_txtName").value, document.getElementById("ctl00_phBody_SearchResult_SearchFeedback_txtEmail").value, document.getElementById("ctl00_phBody_SearchResult_SearchFeedback_txtPhone").value, document.getElementById("ctl00_phBody_SearchResult_SearchFeedback_txtDescription").value, "0", "A79A23F7-8252-4BC2-8804-09B888F0DB57", "dan brown", "/search-books/dan-brown", "Search", "122.177.180.57", AddSearchFeedback_Callback);
           
            return true;
        }
        else {
           
            return false;
        }
    }
    function AddSearchFeedback_Callback(res) {
        if (res != null) {
            document.getElementById("lblmsgSFB").innerHTML = "Thank you for providing the information, we will contact you very soon.";
            document.getElementById("lblmsgSFB").style.display = 'block';
            document.getElementById("ctl00_phBody_SearchResult_SearchFeedback_txtName").value = '';
            document.getElementById("ctl00_phBody_SearchResult_SearchFeedback_txtPhone").value = '';
            document.getElementById("ctl00_phBody_SearchResult_SearchFeedback_txtEmail").value = '';
            document.getElementById("ctl00_phBody_SearchResult_SearchFeedback_txtDescription").value = '';
            document.getElementById('btnAdd').disabled = false;
            
        }
    }
  
</script>


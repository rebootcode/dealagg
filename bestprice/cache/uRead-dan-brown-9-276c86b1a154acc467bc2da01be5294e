<script type="text/javascript">
    var countCallService = 0;
    var totalQty = 0;
    var itemCount = 0;
    var isValidate = true;
    function UpdateBasketItemQty() {
        totalQty = 0;
        countCallService = 0;
        var lblmsg = document.getElementById("lblPopupMsg");
        lblmsg.innerHTML = '';
        validateBasketQty();
        return true;
//        if (validateBasketQty() == true) {
//            var BasketIDs = document.getElementById("hdnBasketID").value;
//            var MFactors = document.getElementById("hdnMF").value;
//            var ProductIDs = document.getElementById("hdnProductID").value;
//            if (BasketIDs.indexOf(",") != -1) {

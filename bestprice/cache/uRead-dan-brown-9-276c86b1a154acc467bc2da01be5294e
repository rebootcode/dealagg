<script type="text/javascript">
    function AddFooterFeedback() {
        
        if (Page_ClientValidate("valFeedback")) {
            document.getElementById('btnAddFB').disabled = true;
            SearchResultService.AddSearchSuggestion(document.getElementById("ctl00_Footer_txtNameFB").value, document.getElementById("ctl00_Footer_txtEmailFB").value, document.getElementById("ctl00_Footer_txtPhoneFB").value, document.getElementById("ctl00_Footer_txtDescriptionFB").value, "0", "A79A23F7-8252-4BC2-8804-09B888F0DB57", "", "", "Footer","", AddFooterFeedback_Callback);
            return true;
        }
        else {

            return false;
        }
    }
    function AddFooterFeedback_Callback(res) {
        if (res != null) {
            document.getElementById("lblmsgFB").innerHTML = "Thank you for your feedback. We will contact you very soon.";
            document.getElementById("lblmsgFB").style.display = 'block';
            document.getElementById("ctl00_Footer_txtNameFB").value = '';
            document.getElementById("ctl00_Footer_txtPhoneFB").value = '';
            document.getElementById("ctl00_Footer_txtEmailFB").value = '';
            document.getElementById("ctl00_Footer_txtDescriptionFB").value = '';
            document.getElementById('btnAddFB').disabled = false;
        }
    }
    function ToggleFooterFBForm() {
        if (document.getElementById("divfooterfeedback").style.display == 'none')
            document.getElementById("divfooterfeedback").style.display = 'block';
        else {
            document.getElementById("divfooterfeedback").style.display = 'none'
        }
    }
    function clearfield() {
       
        document.getElementById("ctl00_Footer_txtNameFB").value = '';
        document.getElementById("ctl00_Footer_txtPhoneFB").value = '';
        document.getElementById("ctl00_Footer_txtEmailFB").value = '';
        document.getElementById("ctl00_Footer_txtDescriptionFB").value = '';
        document.getElementById("lblmsgFB").style.display = 'none';
    }
</script>



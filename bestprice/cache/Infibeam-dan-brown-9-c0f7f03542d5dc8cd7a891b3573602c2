<script>
$(document).ready(function(){
$('.scrollup').fadeOut();
$(window).scroll(function(){
if ($(this).scrollTop() > 400) {
$('.scrollup').fadeIn();
} else {
$('.scrollup').fadeOut();
}
});
$('.scrollup').click(function(){
$("html, body").animate({ scrollTop: 0 }, 1000);
return false;
});
});
</script>

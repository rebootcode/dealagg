<style>
.scrollup{
position:fixed;
bottom:10px;
right: 20px;
}
.scrollup {
background:url(/assets/skins/newcommon/images/icon/sactive.png) no-repeat 0px 0px;
height:60px;
width:58px;
display:block;
}
a.scrollup:hover{
background:url(/assets/skins/newcommon/images/icon/shover.png) no-repeat 0px 0px;
}
</style>
<script>
$(document).ready(function(){
$('.scrollup').fadeOut();
$(window).scroll(function(){
if ($(this).scrollTop() > 400) {
$('.scrollup').fadeIn();
} else {
$('.scrollup').fadeOut();
}
});
$('.scrollup').click(function(){
$("html, body").animate({ scrollTop: 0 }, 1000);
return false;
});
});
</script>

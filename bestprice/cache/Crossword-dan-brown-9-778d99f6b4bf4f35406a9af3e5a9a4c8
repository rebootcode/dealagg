    <script type="text/javascript">
      $( function() {
        $(".navtree").sDrop();


        $("#search input[type=text]").autocomplete({
          source:function( request, response ) {
            $.ajax({
              url: "http://autocomplete_new.infibeam.com/auto_complete/suggest",
              dataType: "jsonp",
              data: {
                term: request.term,
                pc: "books"
              },
              success: function( data ) {
                response(data)
              }
            })
          },
           select: function(event, ui) { 
               $("#search input.text").val(ui.item.value);
               $("#search").trigger("submit");
           }
        }).keydown(function(event) {
           if (event.keyCode=='13'){
            $("#search").trigger("submit");
            }
           });
                
        $("#search select").change(function(){
          var cat = $(this).val().toLowerCase();
          if( cat == "books" || cat == "home")
          {
            $("#search input[type=text]").autocomplete("enable");
          }
          else
          {
            $("#search input[type=text]").autocomplete("disable");        
          }
         });
        $("#search select").change();

        $("#search").submit(function() {
          var href="/"+$("#search select").val() + "/search?q="+encodeURIComponent($("#search input.text").val());
          document.location=href;
          return false;
        });
    
        $(document).keypress(function(event) {
          if(event.which == 57 && event.ctrlKey ){
            $("#bd").toggleClass("showgrid");
          }
        });
        
      });

      $(function() {

      var q="dan brown";
      $("#search input.text").val(q);




    $(".variant-title a").each(function(){
      $(this).data('title',$(this).html());
    })
    $(".contributors a").each(function(){
      $(this).data('ctbr',$(this).html());
    })
  });

    </script>

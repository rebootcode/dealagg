// Pin location finder for pin pointed search result
function chklocationpin(id,boxid){
	var pincode=$('#'+id).val();
	 if(pincode=="" || pincode=="Type Your PIN"){
	 		$('#'+boxid+' .pinfindermsg').html('<span class="errorMsg">Please enter your ZIP code</span>');

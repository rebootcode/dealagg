<script>
					function chklocation(id){
					 var pincode=$('#'+id).val();
					 if(pincode=="" || pincode=="Type Your PIN"){
					 		$('#'+id+'_errorMsg').text('Please enter your ZIP code!');
							$('#'+id).addClass('onError').focus();
							$('#'+id).val('Type Your PIN');
							return false;
					 } else if(!intnum.test(pincode)) {
					 		$('#'+id+'_errorMsg').text('Please enter only numbers!');
							$('#'+id).addClass('onError').focus();
							$('#'+id).val('Type Your PIN');
							return false;
					 } else {
						 $('#'+id+'_errorMsg').text('');
						 $('#overeffect').show().css('height', $(document).height()+'px');
						 $('#updating').show();
						 var SubmitURL = $('#checkLocationReqForm').attr('action');
						 var dataString = 'productId='+ $('#productID').val() + '&pinNumber=' + pincode;
						 $.ajax({type: "POST",url: SubmitURL,data: dataString,success: function(sval)
			  				 {
			  				 	//alert('start'+sval+'end');
			  				 
			    					if(sval=='true'){$('#'+id+'_successMsg').text('We ship this product to -'+pincode);}
			    					else{$('#'+id+'_errorMsg').text('We do not ship this product to -'+pincode);}
			    				
			  				 },error:function()
								 {$('#'+id+'_errorMsg').text('We do not ship this product to -'+pincode);}
							 });
						 $('#updating, #overeffect').hide();
						 return false;
					 }				
					}
</script>
<script>
function replace(obj){
//alert("original image was changed.. this is a new image....")
obj.src="/images/defaultImage.jpg";
}
</script>

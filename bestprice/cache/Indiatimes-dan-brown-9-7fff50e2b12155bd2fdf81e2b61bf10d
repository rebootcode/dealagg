<script type="text/javascript">
// Pin location finder for pin pointed search result
function chklocationpin(id,boxid){
	var pincode=$('#'+id).val();
	 if(pincode=="" || pincode=="Type Your PIN"){
	 		$('#'+boxid+' .pinfindermsg').html('<span class="errorMsg">Please enter your ZIP code</span>');
			$('#'+id).addClass('onError').focus();
			$('#'+id).val('Type Your PIN');
			return false;
	 } else if(!intnum.test(pincode)) {
	 		$('#'+boxid+' .pinfindermsg').html('<span class="errorMsg">Please enter only numbers</span>');
			$('#'+id).addClass('onError').focus();
			$('#'+id).val('Type Your PIN');
			return false;
	 }
	 else {
		 $('#'+id).removeClass('onError');
		 $('#overeffect').show().css('height', $(document).height()+'px');
		 $('#updating').show();
		 var SubmitURL = $('#checkLocationReqForm').attr('action');
		 var dataString = 'productId='+ $('#upsell'+id).val() + '&pinNumber=' + pincode ;
		 $.ajax({type: "POST",url: SubmitURL,data: dataString,success: function(sval)
				 {
			 		$('#'+boxid+' .pinfindermsg').html(sval);
				 },error:function(sval)
				 {
				 	alert(sval);
					 $('#'+boxid+' .pinfindermsg').html('<span class="errorMsg">We do not ship this product to -'+pincode+'</span>');
				 }
			 });
		 $('#updating, #overeffect').hide();
		 return false;
		 
	 }
}
</script>
<!-- End Template component://order/webapp/ordermgr/entry/catalog/pinpointedproductsummary.ftl -->

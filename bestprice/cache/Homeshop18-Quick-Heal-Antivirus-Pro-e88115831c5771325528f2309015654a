<!-- End Google Tag Manager --> <script type="text/javascript" language="javascript">
    var inputMinPrice = 0.0;
    var inputMaxPrice = 0.0;

    var searchMinPrice = 0.0;
    var searchMaxPrice = 0.0;

    if(inputMinPrice == 0 && inputMaxPrice == 0){
     $('#priceSlider').hide();
    }
    setPrice(inputMinPrice,inputMaxPrice);

    function setPrice(minPrice, maxPrice){
        $( "#amountMin" ).text( minPrice );
        $( "#amountMax" ).text( maxPrice );
    }

    function logClick(userQuery,productId,position,sessionId){
         var date = new Date();
         date.setTime(date.getTime()+(3*60*1000));
         var expires = " expires="+date.toGMTString();
         var jsonobj="{'searchQuery':'"+userQuery+"','clickedItem':"+productId+",'position':"+position+",'sessionid':'"+sessionId+"'}";
         document.cookie = "clickItemCookie=\""+jsonobj+"\";"+expires+";"+"path=/";
         return true;
     }
    function openSorted(sortselect)
    {
        var selVal;

        selVal = sortselect.options[sortselect.selectedIndex].value;
        if(selVal == 'Price Low-to-High')
        {
            window.location.href = 'http://www.homeshop18.com/quick-heal-antivirus-pro/search:Quick+Heal+Antivirus+Pro/sort:Price+Low-to-High/start:0/';
        }
        else if(selVal == 'Price High-to-Low')
        {
            window.location.href = 'http://www.homeshop18.com/quick-heal-antivirus-pro/search:Quick+Heal+Antivirus+Pro/sort:Price+High-to-Low/start:0/';
        }
        else
        {
            window.location.href = 'http://www.homeshop18.com/quick-heal-antivirus-pro/search:Quick+Heal+Antivirus+Pro/sort:score/start:0/';
        }
    }

    function priceChange(minPrice,maxPrice){
        var prcString = '['+ minPrice +' TO '+maxPrice+']';
        prcString = "/prc:"+escape(prcString);
        var searchPriceRange = '['+ searchMinPrice +' TO '+ searchMaxPrice+']';
        searchPriceRange = "/searchprc:"+escape(searchPriceRange);
        var toUrl = "";
        var currentUrl = window.location.href;             
        if(currentUrl.indexOf('prc:')!=-1){
            //eiter price string ends with / or $
            toUrl = currentUrl.replace(/\/prc:.*?(\/|$)/,prcString+"/");           
        }
        else{
            toUrl = window.location.href+prcString+searchPriceRange;
        }
        toUrl=toUrl.replace(/start:[0-9]+/,'start:0');  
        //console.log(toUrl);        
        window.location.href=toUrl;
    }


    $(document).ready(function() {
        $( "#slider-range" ).slider({
            range: true,
            min: searchMinPrice,
            max: searchMaxPrice,
            values: [inputMinPrice, inputMaxPrice],
            slide: function( event, ui ) {
                setPrice(ui.values[0],ui.values[1]);
            },
            change: function(event, ui) {
                priceChange(ui.values[0],ui.values[1]);
            }
        });

        $("#tree").treeview({
            collapsed:true
        });
        $("input[type=checkbox]").click(function(){
            var url = $(this).val();
            window.location.href= url;
        });
        $( "#q_category option[value=" +  + "]" ).attr('selected', 'selected').trigger('change');     
        
        $( ".filter_header span" ).click(function () {
            var id = $(this).attr('id');
            $(".toggle_"+id).toggle();
            
            if ($(this).hasClass('on')) {
                $(this).removeClass('on').addClass('off');
                $(this).parent().css("margin-bottom","10px");                    
            } else {
                $(this).removeClass('off').addClass('on');
                $(this).parent().css("margin-bottom","0px");
            } 
        });
        
    });	
  </script>
</body>

<script type="text/javascript">
    	$(document).ready(function() {
            // Quick View
            $(".product_div").hover(
                function () {
                   //$(this).find(".quicklook").css("display", "block");
                   $(this).find(".quicklook").fadeIn("slow");
                },
                function () {
                    $(this).find(".quicklook").css("display", "none");
